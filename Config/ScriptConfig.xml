<?xml version="1.0" encoding="utf-8"?>
<!-- Default Preset value for Dell laptop used in Production-->
<ScriptConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
<Profiles>Workstation</Profiles>
<Workstation>SGSI11-59FKK13</Workstation>
<Drive>S:</Drive>
<path>S:\RHID</path>
<Danno>S:\Dano Planning\Test Data\</Danno>
<US_Drive>Y:</US_Drive>
<US_Path>Y:\RHID</US_Path>
<US_danno>Y:\Dano Planning\Test Data\</US_danno>

<!--  Default values for Instruments and the rest of the Workstation PCs -->
<Profiles>Instruemnts</Profiles>
<Workstation>Instruments</Workstation>
<Drive>U:</Drive>
<path>U:\RHID</path>
<danno>U:\Dano Planning\Test Data\</danno>

<!-- Script-wide Variables -->
<HostName>$env:COMPUTERNAME</HostName>
<SystemTimeZone>[System.TimeZoneInfo]::Local.DisplayName</SystemTimeZone>
<PST_TimeZone>[System.TimeZoneInfo]::ConvertTimeBySystemTimeZoneId([DateTime]::Now, "Pacific Standard Time")</PST_TimeZone>
<InteralDisplay>CHR $env:COMPUTERNAME (Internal)</InteralDisplay>
<DELL_Display>DEL $env:COMPUTERNAME (VGA)</DELL_Display>
<SerialRegMatch>"$HostName" -match "RHID-\d\d\d\d"</SerialRegMatch>
<GetDate>${get-date} = Get-date</GetDate>
<Inst_rhid_Folder>"E:\RapidHIT ID"</Inst_rhid_Folder>
<Inst_rhid_Result>"E:\RapidHIT ID\Results"</Inst_rhid_Result>
<Nonlinearity_File>"Non-linearity Calibration $HostName.PNG"</Nonlinearity_File>
<Waves_File>"Waves $HostName.PNG"</Waves_File>
<TC_verification_File>"TC_verification $HostName.TXT"</TC_verification_File>

<TC_CalibrationXML>TC_Calibration.XML</TC_CalibrationXML>
<OverrideSettings>OverrideSettings.XML</OverrideSettings>
<DannoAppConfig>DannoAppConfig.xml</DannoAppConfig>
<MachineConfig>MachineConfig.xml</MachineConfig>
<StatusData>StatusData_Graphs.pdf</StatusData>
<GM_Analysis>GM_Analysis.sgf</GM_Analysis>
<TC_CalibrationXML_File>TC_Calibration.xml</TC_CalibrationXML_File>
<DannoAppConfigXML_File>DannoAppConfig.xml</DannoAppConfigXML_File>

<Nonlinearity_Leaf>Test-Path -Path $Inst_rhid_Result\$Nonlinearity_File -PathType Leaf</Nonlinearity_Leaf>
<Waves_Leaf>Test-Path -Path $Inst_rhid_Result\$Waves_File -PathType Leaf</Waves_Leaf>
<TC_verification_Leaf>Test-Path -Path $Inst_rhid_Result\$TC_verification_File -PathType Leaf</TC_verification_Leaf>
<MachineConfig_Leaf>Test-Path -Path $Inst_rhid_Folder\$MachineConfig_File -PathType Leaf</MachineConfig_Leaf>
<TC_CalibrationXML_Leaf>Test-Path -Path $Inst_rhid_Folder\$TC_CalibrationXML_File -PathType Leaf</TC_CalibrationXML_Leaf>
<DannoAppConfigCheck>Test-Path -Path "E:\RapidHIT ID\Results\Data $HostName\DannoAppConfig.xml" -PathType Leaf</DannoAppConfigCheck>
<DannoAppRhidCheck>Test-Path -Path "D:\DannoGUI\DannoAppConfig.xml" -PathType Leaf</DannoAppRhidCheck>

<internal>Test-Path -Path "U:\$HostName\Internal\"</internal>
<US_internal>Test-Path -Path "Y:\$HostName\Internal\"</US_internal>
<Danno_leaf>Test-Path -Path "U:\Dano Planning\Test Data\$HostName"</Danno_leaf>
<US_Danno_leaf>Test-Path -Path "Y:\Dano Planning\Test Data\$HostName"</US_Danno_leaf>

<RealtimeProtection>Get-MpPreference | select-object DisableRealtimeMonitoring</RealtimeProtection>

<Debug>off</Debug>
<ExitCode>$Null</ExitCode>
<VerboseMode>True</VerboseMode>
<HistoryMode>False</HistoryMode>
</ScriptConfig>